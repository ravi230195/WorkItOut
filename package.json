{
  "name": "workout-tracker",
  "version": "1.0.0",
  "description": "A workout tracker app with warm pastel design and native iOS capabilities",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "setup:ios": "./setup-ios-direct.sh",
    "ios:setup": "npx cap init 'Workout Tracker' 'com.workouttracker.app' && npx cap add ios",
    "ios:sync": "npm run build && npx cap sync ios",
    "ios:build": "npm run ios:sync && ./ios/build.sh",
    "ios:install": "npm run ios:sync && ./ios/install-on-device.sh",
    "ios:open": "npx cap open ios",
    "mobile:setup": "./setup-mobile.sh",
    "cap:sync": "npx cap sync",
    "cap:open": "npx cap open"
  },
